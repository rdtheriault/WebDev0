<script>
    for (var i=0, i<10;i+=2){
        document.getElementById("one")innerHTML+=(i+1)*2+"<br>";    
    }    
    document.getElementById("two")innerHTML="<ul><li>Item1</li><li>Item2</li><li>Item3</li></ul>";
document.getElementById("btn").addEventListener("click",doSomething);

function doSomething() {
  var input=document.getElementById=("in").value;
    document.getElementById("three")innerHTML="<h1>"+input+"</h1>";
     document.getElementById("three").style.backgroundColor="cyan";
     document.getElementById("link").href="https://yahoo.com";
  if(input==="10"){
  document.getElementById("three").innerHTML="That was ten";
  }

}
